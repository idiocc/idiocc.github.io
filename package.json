  "author": "{{ author_name }} <{{ author_email }}>",
  "license": "{{ license_spdx }}",
  "bugs": {
    "url": "{{ repo.html_url }}/issues"
  },
  "homepage": "{{ URL }}",
  "devDependencies": {
    "documentary": "^1.37.1",
    "splendid": "^1.17.3",
    "zoroaster": "^4.3.0"
