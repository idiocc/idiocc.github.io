{
  "name": "my-new-package",
  "version": "0.0.0",
  "description": "{{ description }}",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "doc": "doc -o README.md -d",
    "splendid": "splendid",
    "prod": "splendid -s",
    "dev": "splendid -s -e dev",
    "s": "splendid -s",
    "d": "splendid -s -e dev"
  },
  "files": [],
  "repository": {
